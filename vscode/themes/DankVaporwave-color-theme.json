  "name": "DankVaporwave",
  "type": "dark",
  "colors": {
    "editor.background": "#0d1117",
    "editor.foreground": "#e74a98",
    "statusBar.background": "#9f1c5e",
    "sideBarTitle.foreground": "#e74a98",
    "sideBar.background": "#0d1117",
    "sideBar.border": "#32383a",
    "activityBar.background": "#0d1117",
    "tab.activeForeground": "#b392f0",
    "tab.activeBorderTop": "#b392f0",
    "tab.inactiveForeground": "#7c8484",
    "tab.inactiveBackground": "#0d1117",
    "titleBar.activeBackground": "#0d1117",
    "titleBar.inactiveBackground": "#0d1117",
    "editorGroupHeader.tabsBackground": "#0d1117",
    "badge.background": "#e74a98",
    "badge.foreground": "#f0f6fc",
    "activityBarBadge.background": "#802a55",
    "activityBarBadge.foreground": "#f0f6fc",
    "list.activeSelectionBackground": "#0d1117",
    "list.activeSelectionForeground": "#f0f6fc",
    "list.focusOutline": "#b392f0",
    "list.inactiveSelectionBackground": "#7c848445",
    "searchEditor.findMatchBackground": "#802a55",
    "editor.findMatchBackground": "#802a55",
    "editor.findMatchHighlightBackground": "#802a55",
    "editor.findRangeHighlightBackground": "#802a55",
    "editor.lineHighlightBackground": "#802a5521",
    "editor.selectionBackground": "#802a55",
    "extensionBadge.remoteBackground": "#802a55",
    "extensionButton.background": "#802a55",
    "sideBarSectionHeader.foreground": "#f0f6fc",
    "sideBar.dropBackground": "#b392f0",
    "panelInput.border": "#b392f0",
    "button.background": "#e74a98",
    "editorWidget.resizeBorder": "#b392f0",
    "keybindingLabel.background": "#e74a98",
    "foreground": "#f0f6fc",
    "menu.selectionBackground": "#e74a98",
    "selection.background": "#e74a98",
    "dropdown.border": "#b392f0",
    "focusBorder": "#b392f0"
  },

  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#546E7A"
      }
    },
    {
      "name": "Import names",
      "scope": [
        "variable.other.readwrite.alias",
        "string.quoted.single.ts",
        "variable.other.readwrite.ts",
        "meta.import.ts"
      ],
      "settings": {
        "foreground": "#f0f6fc"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "constant.other.placeholder", "variable.other.object.ts"],
      "settings": {
        "foreground": "#f0f6fc"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.modifier"],
      "settings": {
        "foreground": "#e74a98"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": ["keyword.control"],
      "settings": {
        "foreground": "#e74a98"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "constant.other.color",
        "punctuation",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#f0f6fc"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag", "meta.tag.sgml", "markup.deleted.git_gutter"],
      "settings": {
        "foreground": "#b392f0"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#b392f0"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": ["meta.block", "variable.other"],
      "settings": {
        "foreground": "#f0f6fc"
      }
    },
    {
      "name": "Method Variables Definition",
      "scope": [
        "variable.other.constant.ts",
        "meta.definition.variable.ts",
        "meta.method.declaration.ts",
        "variable.other.object.ts",
        "variable.other"
      ],
      "settings": {
        "foreground": "#f0f6fc"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable", "string.other.link"],
      "settings": {
        "foreground": "#b392f0"
      }
    },
    {
      "name": "Instance variable",
      "scope": ["support.variable.property.ts"],
      "settings": {
        "foreground": "#b2deff"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#b392f0"
      }
    },
    {
      "name": "Constant",
      "scope": ["keyword", "constant.language", "variable.other"],
      "settings": {
        "foreground": "#b2deff"
      }
    },
    {
      "name": "Class fields",
      "scope": ["variable.object.property.ts", "meta.class.ts", "entity.name.type.interface.ts"],
      "settings": {
        "foreground": "#b2deff"
      }
    },
    {
      "name": "String quote",
      "scope": ["string.quoted.single.ts", "string.quoted.double.ts"],
      "settings": {
        "foreground": "#b2deff"
      }
    },
    {
      "name": "Number, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "keyword.operator.expression",
        "constant.numeric",
        "support.constant",
        "constant.character",
        "constant.escape",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#e74a98"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#b2deff"
      }
    },
    {
      "name": "Return Types",
      "scope": ["support.type.builtin.ts", "meta.type.parameters.ts"],
      "settings": {
        "foreground": "#b2deff"
      }
    },
    {
      "name": "Parameters",
      "scope": ["meta.parameters.ts", "meta.interface.ts"],
      "settings": {
        "foreground": "#f0f6fc"
      }
    },
    {
      "name": "Support Types",
      "scope": ["meta.return.type.ts", "meta.method.declaration.ts", "meta.interface.ts", "source.ts"],
      "settings": {
        "foreground": "#e74a98"
      }
    },
    {
      "name": "Class",
      "scope": ["storage.type"],
      "settings": {
        "foreground": "#e74a98"
      }
    },
    {
      "name": "Support",
      "scope": [
        "support.other.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "#b392f0"
      }
    },

    {
      "name": "Support Types Primitive",
      "scope": ["support.type.primitive.ts"],
      "settings": {
        "foreground": "#f0f6fc"
      }
    },
    {
      "name": "Entity name class / Type",
      "scope": ["entity.name.type.alias.ts", "entity.name.function.ts"],
      "settings": {
        "foreground": "#f0f6fc"
      }
    },
    {
      "name": "Meta name type",
      "scope": ["meta.type.annotation.ts", "meta.method.declaration.ts"],
      "settings": {
        "foreground": "#B2DEFF"
      }
    },

    {
      "name": "Variable Parameter",
      "scope": ["variable.parameter.ts"],
      "settings": {
        "foreground": "#f0f6fc"
      }
    },
    {
      "name": "Function call",
      "scope": ["entity.name.function.ts", "meta.function-call.ts"],
      "settings": {
        "foreground": "#b392f0"
      }
    },
    {
      "name": "Support Types",
      "scope": ["support.type.builtin.ts"],
      "settings": {
        "foreground": "#e74a98"
      }
    },
    {
      "name": "Null Types",
      "scope": ["constant.language.null.ts"],
      "settings": {
        "foreground": "#e74a98"
      }
    },
    {
      "name": "Comparison",
      "scope": ["keyword.operator.comparison.ts"],
      "settings": {
        "foreground": "#B2DEFF"
      }
    },
    {
      "name": "Entity name class / Type",
      "scope": ["punctuation.definition.typeparameters.end.ts", "punctuation.definition.typeparameters.begin.ts"],
      "settings": {
        "foreground": "#B2DEFF"
      }
    },
    {
      "name": "Function Punctuation",
      "scope": ["storage.type.function.arrow.ts", "punctuation.definition.block.ts"],
      "settings": {
        "foreground": "#d3a4e3"
      }
    },
    {
      "name": "This Keyword",
      "scope": ["variable.language.this.ts"],
      "settings": {
        "foreground": "#d3a4e3"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#B2CCD6"
      }
    },
    {
      "name": "Sub-methods",
      "scope": ["entity.name.module.js", "variable.import.parameter.js", "variable.other.class.js"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Language methods",
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#b392f0"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": ["entity.name.method.js"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "meta.method.js",
      "scope": ["meta.class-method.js entity.name.function.js", "variable.function.constructor"],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#b392f0"
      }
    },

    // Markup
    {
      "name": "HTML Attributes",
      "scope": ["text.html.basic entity.other.attribute-name.html", "text.html.basic entity.other.attribute-name"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "CSS Classes",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "CSS ID's",
      "scope": ["source.sass keyword.control"],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#b392f0"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Decorators",
      "scope": ["tag.decorator.js entity.name.tag.js", "tag.decorator.js punctuation.definition.tag.js"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": ["source.js constant.other.object.key.js string.unquoted.label.js"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF5370"
      }
    },
